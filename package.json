{
  "name": "voting-app",
  "version": "1.0.0",
  "description": "E-Voting application",
  "author": "Avrora",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prepare": "husky",
    "prisma:client:generate": "npx prisma generate",
    "prisma:execute": "npx prisma db execute --schema prisma/schema.prisma",
    "prisma:migrate:dev": "npx prisma migrate dev",
    "prisma:migrate:prod": "npx prisma migrate deploy",
    "prisma:migration:generate:down": "npx prisma migrate diff --from-schema-datamodel prisma/schema.prisma --to-schema-datasource prisma/schema.prisma --script > prisma/migrations/down.sql",
    "prisma:migration:generate:up": "npx prisma migrate dev --create-only",
    "prisma:resolve": "npx prisma migrate resolve",
    "prisma:seed": "npx prisma db seed",
    "prisma:status": "npx prisma migrate status",
    "prisma:studio": "npx prisma studio",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "yarn format",
      "yarn lint",
      "bash -c tsc --noEmit -p ./tsconfig.json",
      "git add --force"
    ],
    "*.json": [
      "yarn format",
      "git add --force"
    ]
  },
  "prisma": {
    "seed": "ts-node prisma/seeders/main-seed.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^9.4.0",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.0.7",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.7",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.0.7",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.0.3",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.3.0",
    "axios": "^1.8.1",
    "bullmq": "^5.39.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cron-parser": "^4.9.0",
    "dayjs": "^1.11.13",
    "express-basic-auth": "^1.2.1",
    "fast-safe-stringify": "^2.1.1",
    "fido2-lib": "^3.5.3",
    "ioredis": "^5.4.2",
    "joi": "^17.13.3",
    "ms": "^2.1.3",
    "nest-winston": "^1.10.2",
    "pluralize": "^8.0.0",
    "prisma-extension-soft-delete": "^2.0.0",
    "puppeteer": "^24.10.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "uuid": "^11.0.5",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@darraghor/eslint-plugin-nestjs-typed": "^6.0.0",
    "@eslint/compat": "^1.2.6",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.19.0",
    "@golevelup/ts-jest": "^0.6.2",
    "@nestjs/cli": "^11.0.2",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.7",
    "@types/express": "^5",
    "@types/jest": "^29.5.14",
    "@types/ms": "^2.1.0",
    "@types/node": "^22.13.0",
    "@types/pluralize": "^0.0.33",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "eslint": "^9.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.3",
    "prettier": "^3.4.2",
    "prisma": "^6.3.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0"
  },
  "engines": {
    "node": "^22.13.1",
    "npm": "^11.1.0",
    "yarn": "^1.22.22"
  },
  "jest": {
    "moduleNameMapper": {
      "^@common/(.*)$": "<rootDir>/common/$1",
      "^@auth/(.*)$": "<rootDir>/../src/modules/auth/$1",
      "^@otp/(.*)$": "<rootDir>/../src/modules/otp/$1",
      "^@core/(.*)$": "<rootDir>/core/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
